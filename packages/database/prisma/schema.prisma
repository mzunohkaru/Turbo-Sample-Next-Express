generator client {
  provider        = "prisma-client-js"
  binaryTargets   = "native"
  engineType = "binary"
  previewFeatures = ["views"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(cuid())
  email String @unique
  name  String

  @@map("users")
}
